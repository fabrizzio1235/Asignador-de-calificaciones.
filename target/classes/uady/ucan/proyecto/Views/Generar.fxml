<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<VBox alignment="CENTER" prefHeight="250" prefWidth="400" spacing="20"  xmlns:fx="http://javafx.com/fxml/1" fx:controller="uady.ucan.proyecto.CambioDeMenu">
    <stylesheets>
        <URL value="@Styles/menu.css"/>
    </stylesheets>
    <HBox alignment="CENTER" spacing="5">
        <Label text="Nombre del CSV:"/>
        <TextField fx:id="csvNombre"></TextField> <Label text=".csv" styleClass="label-terminacion"/>
    </HBox>
    <HBox alignment="CENTER" spacing="5">
        <Button text="Confirmar" onAction="#generarCSV"/>
    </HBox>
    <HBox alignment="CENTER" spacing="5">
        <Label text="Nombre del PDF:"/>
        <TextField fx:id="pdfNombre"></TextField> <Label text=".pdf" styleClass="label-terminacion"/>
    </HBox>
    <HBox alignment="CENTER" spacing="5">
        <Button text="Confirmar" onAction="#generarPDF"/>
    </HBox>
</VBox>
